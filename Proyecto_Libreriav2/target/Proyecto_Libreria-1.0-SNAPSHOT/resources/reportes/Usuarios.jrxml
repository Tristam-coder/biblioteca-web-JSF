	<parameter name="TIPO_USUARIO" class="java.lang.String">
		<defaultValueExpression><![CDATA["TODOS"]]></defaultValueExpression>
	</parameter>
	<parameter name="ESTADO_USUARIO" class="java.lang.String">
		<defaultValueExpression><![CDATA["TODOS"]]></defaultValueExpression>
	</parameter>
	<query language="sql"><![CDATA[SELECT 
            u.id,
            u.dni,
            u.nombre,
            u.apellido,
            CONCAT(u.nombre, ' ', u.apellido) as nombre_completo,
            u.email,
            u.telefono,
            u.direccion,
            u.tipo_usuario,
            u.estado,
            u.fecha_registro,
            COUNT(DISTINCT p.id) as total_prestamos,
            COUNT(DISTINCT CASE WHEN p.estado = 'ACTIVO' THEN p.id END) as prestamos_activos,
            COUNT(DISTINCT r.id) as total_reservas,
            COALESCE(SUM(m.monto), 0) as total_multas,
            COALESCE(SUM(CASE WHEN m.estado = 'PENDIENTE' THEN m.monto ELSE 0 END), 0) as multas_pendientes
        FROM usuario u
        LEFT JOIN prestamo p ON u.id = p.usuario_id
        LEFT JOIN reserva r ON u.id = r.usuario_id AND r.estado = 'PENDIENTE'
        LEFT JOIN multa m ON u.id = m.usuario_id
        WHERE ($P{TIPO_USUARIO} = 'TODOS' OR u.tipo_usuario = $P{TIPO_USUARIO})
        AND ($P{ESTADO_USUARIO} = 'TODOS' OR u.estado = $P{ESTADO_USUARIO})
        GROUP BY u.id, u.dni, u.nombre, u.apellido, u.email, u.telefono, u.direccion, 
                 u.tipo_usuario, u.estado, u.fecha_registro
        ORDER BY u.apellido, u.nombre]]></query>
